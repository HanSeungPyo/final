	function check() {
		var v = document.getElementById("allCheck"); /* 전체동의 */
		var v1 = document.getElementById("check1");
		var v2 = document.getElementById("check2");
		var v3 = document.getElementById("check3");
		
		if (v.checked == true) {
			v1.checked = "checked";
			v2.checked = "checked";
			v3.checked = "checked";
		} else {
			v1.checked = false;
			v2.checked = false;
			v3.checked = false;
		}
	}

	function checkAll() {
		var v1 = document.getElementById("check1");
		var v2 = document.getElementById("check2");
		var v3 = document.getElementById("check3");

		if (v1.checked == true && v2.checked == true && v3.checked == true) {
			location.href = "me_join.jsp";
		} else {
			alert("전체 동의해주세요");
			return false;
		}
	}

	function checkTest() {
		var v = document.getElementById("allCheck"); /* 전체동의 */
		var v1 = document.getElementById("check1");
		var v2 = document.getElementById("check2");
		var v3 = document.getElementById("check3");

		if (!v1.checked || !v2.checked || !v3.checked) {
			v.checked = false;
		}
		if (v1.checked && v2.checked && v3.checked) {
			v.checked = true;
		}
	}
